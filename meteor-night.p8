--meteor night
--by tobiasvl
function _init()
  -- tweak these variables
  debug=false
  chill_factor=0.003
  canvas=2000
  stars_amount=canvas*2

  cartdata("tobiasvl_meteor_night")
  last_mouse,last_x,last_y=0,0,0
  s=cocreate(say)
  meteors={}
  music(0)
  states={
    intro=0,
    title=1,
    game_over=2
  }
  sunset_colors={
    --12,
    {9,11},
    {14,11},
    {8,3},
    {2,3},
    --13,
    --1,
    {0,13}
  }
  state=states.intro

  cam_x,cam_y=canvas/2,canvas/2
  frame_counter=0
  counter=1
  credits_y=0

  chilly=true
  chill=0

  meteors_seen=0
  meteors_high=dget(1)
  poke(0x5f2d,1)
  mouse={x=stat(32),y=stat(33)}
  spr_x=40

  stars={}
  for i=1,stars_amount do
    local x,y=flr(rnd(canvas)),flr(rnd(canvas))
    add(stars,{x=x,y=y})
    if x<128 then
      add(stars,{x=x+canvas,y=y})
    elseif y<128 then
      add(stars,{x=x,y=y+canvas})
    end
    if x<128 and y<128 then
      add(stars,{x=x+canvas,y=y+canvas})
    end
  end

  moon={x=flr(rnd(canvas)),y=flr(rnd(canvas))}
end
