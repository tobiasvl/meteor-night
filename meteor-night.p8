--update
function _update()
  local mouse_pressed=stat(34)==1

  local mouse_released=false
  if mouse.down and not mouse_pressed then
    mouse.down=false
    mouse_released=true
  else
    mouse.down=mouse_pressed
  end

  local temp_mouse=mouse_pressed
  mouse_pressed=mouse_pressed!=last_mouse and mouse_pressed or false
  last_mouse=temp_mouse

  if state==states.intro then
    if btnp(‚ùé) or mouse_pressed then
      title_y=cam_y
      state=states.title
    end
  elseif state==states.title then
    if title_y<=0 then
      title_y=canvas
    end

    if (btnp(üÖæÔ∏è)) chilly=not chilly
    if (btnp(‚ùé) or mouse_pressed) scroll=true

    if scroll then
      title_y-=1
    --if (cam_y<-60) coresume(s,"opening",true)
    end

    if scroll and title_y==cam_y-80 then
      scroll=false
      cam_y=title_y
      state=states.play
    end
  elseif state==states.play then
    if cam_x>=canvas then
      cam_x=0
    elseif cam_x<=0 then
      cam_x=canvas
    end
    if cam_y>=canvas then
      cam_y=0
    elseif cam_y<=0 then
      cam_y=canvas
    end

    if costatus(s)=="dead" and flr(rnd(10000))==42 then
      s=cocreate(say)
      coresume(s,"random",true)
    end

    if chilly then
      if chill>30 then
        if costatus(s)=="dead" then
          s=cocreate(say)
          if meteors_seen==0 then
            coresume(s,"home_dissatisfied",false)
          elseif meteors_seen<=5 then
            coresume(s,"home_satisfied",false)
          elseif meteors_seen==meteors_high then
            coresume(s,"home_high",false)
          else
            coresume(s,"home_amazing",false)
          end
        end
      else
        chill+=chill_factor
      end
    end

    local x,y=stat(32),stat(33)
    if x!=old_x or y!=old_y or mouse_pressed then
      old_x,old_y=x,y
      mouse.x,mouse.y=x+cam_x,y+cam_y
    end

    local analog={}
    analog.x,analog.y=analog_x()/5,analog_y()/5
    if analog.x!=0 then
      if mouse.x>=127+cam_x then
        cam_x+=1
        mouse.x+=1
      elseif mouse.x<=1+cam_x then
        cam_x-=1
        mouse.x-=1
      else
        mouse.x+=analog.x
      end
    end
    if analog.y!=0 then
      if mouse.y>=127+cam_y then
        cam_y+=1
        mouse.y+=1
      elseif mouse.y<=1+cam_y then
        cam_y-=1
        mouse.y-=1
      else
        mouse.y+=analog.y
      end
    end

    if analog.x==0 and analog.y==0 then
      if (btnp(‚¨áÔ∏è)) mouse.y+=10
      if (mouse.y>=127+cam_y) cam_y+=1 mouse.y+=1
      if (btnp(‚û°Ô∏è)) mouse.x+=10
      if (mouse.x>=127+cam_x) cam_x+=1 mouse.x+=1
      if (btnp(‚¨ÜÔ∏è)) mouse.y-=10
      if (mouse.y<=1+cam_y) cam_y-=1 mouse.y-=1
      if (btnp(‚¨ÖÔ∏è)) mouse.x-=10
      if (mouse.x<=1+cam_x) cam_x-=1 mouse.x-=1
    end

    if mouse_pressed then
      mouse.pressed_x,mouse.pressed_y=mouse.x,mouse.y
    end

    touch=(mouse_released and mouse.x==mouse.pressed_x and mouse.y==mouse.pressed_y)

    if (btnp(‚ùé) or touch) and costatus(s)=="dead" then
      --[[if mouse.x>=cam_x+40 then
        s=cocreate(say)
        coresume(s,"cold",false)]]
      for meteor_data in all(meteors) do
        if meteor_data.x>=cam_x and meteor_data.x<=cam_x+127 and meteor_data.y>=cam_y and meteor_data.y<=cam_y+127 then
          s=cocreate(say)
          coresume(s,"look",true)
        end
      end
      if mouse.x>=moon.x and mouse.x<=moon.x+16 and mouse.y>moon.y and mouse.y<moon.y and costatus(s)=="dead" then
        s=cocreate(say)
        coresume(s,"moon",true)
      end
      if costatus(s)=="dead" then
        s=cocreate(say)
        coresume(s,"miss",true)
      end
      mouse_pressed=false
    end

    if #meteors<meteors_amount then
      add(meteors,create_meteor())
    end

    if chilly and meteors_seen>meteors_high then
      meteors_high=meteors_seen
      dset(1,meteors_high)
    end
  elseif state==states.game_over then
    if credits_y>=canvas then
      cam_y=-(credits_y-cam_y)
      credits_y=0
    end

    if credits_y<cam_y+660 then
      credits_y+=1
    else
      meteors_seen,chill=0,0
      title_y=credits_y
      cam_y=title_y
      state=states.title
    end
  end
end
