--update
function _update()
  local mouse_pressed=stat(34)==1
  local temp_mouse=mouse_pressed
  mouse_pressed=mouse_pressed!=last_mouse and mouse_pressed or false
  last_mouse=temp_mouse

  if state==states.intro then
    if btnp(‚ùé) or mouse_pressed then
      title_y=cam_y
      state=states.title
    end
  elseif state==states.title then
    if title_y<=0 then
      title_y=canvas
    end
    
    if (btnp(üÖæÔ∏è)) chilly=not chilly
    if (btnp(‚ùé) or mouse_pressed) scroll=true
    
    if scroll then
      title_y-=1
    --if (cam_y<-60) coresume(s,"opening",true)
    end
    
    if scroll and title_y==cam_y-80 then
      scroll=false
      cam_y=title_y
      state=states.play
    end
  elseif state==states.play then
    if cam_x>=canvas then
      cam_x=0
    elseif cam_x<=0 then
      cam_x=canvas
    end
    if cam_y>=canvas then
      cam_y=0
    elseif cam_y<=0 then
      cam_y=canvas
    end
  
    if chilly then
      if chill>30 then
        if costatus(s)=="dead" then
          s=cocreate(say)
          coresume(s,"home",false)
        end
      else
        chill+=chill_factor
      end
    end
    
    mouse.x,mouse.y=stat(32)+cam_x,stat(33)+cam_y
    if (btnp(‚¨áÔ∏è)) mouse.y+=1
    if (mouse.y>=128+cam_y) cam_y+=1
    if (btnp(‚û°Ô∏è)) mouse.x+=1
    if (mouse.x>=128+cam_x) cam_x+=1
    if (btnp(‚¨ÜÔ∏è)) mouse.y-=1
    if (mouse.y<=cam_y) cam_y-=1
    if (btnp(‚¨ÖÔ∏è)) mouse.x-=1
    if (mouse.x<=cam_x) cam_x-=1

    if mouse_pressed and costatus(s)=="dead" then
      --[[if mouse.x>=cam_x+40 then
        s=cocreate(say)
        coresume(s,"cold",false)]]
      for meteor_data in all(meteors) do
        if mouse.x>=meteor_data.x-1500-15 and mouse.x<=meteor_data.x-1500+15 and mouse.y>=meteor_data.y-1500-15 and mouse.y<=meteor_data.y-1500+15 then
          s=cocreate(say)
          coresume(s,"look",true)
        end
      end
      if costatus(s)=="dead" then
        s=cocreate(say)
        coresume(s,"miss",true)
      end
      mouse_pressed=false
    end
    
    if #meteors<100 then
      add(meteors,create_meteor())
    end
    
    if chilly and meteors_seen>meteors_high then
      meteors_high=meteors_seen
      dset(1,meteors_high)
    end
  elseif state==states.game_over then
    if credits_y>=canvas then
      cam_y=-(credits_y-cam_y)
      credits_y=0
    end
    
    if credits_y<cam_y+660 then
      credits_y+=1
    else
      meteors_seen,chill=0,0
      title_y=credits_y
      cam_y=title_y
      state=states.title
    end
  end
end
