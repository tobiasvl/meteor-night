--meteor night
--by tobiasvl
function _init()
  -- tweak these variables
  debug=false
  chill_factor=0.003
  canvas=2000
  stars_amount=canvas*2
  meteors_amount=100

  keyboard=stat(102)!=0 and stat(102)!="www.lexaloffle.com" and stat(102)!="www.playpico.com"
  buttons={o=keyboard and "c" or "üÖæÔ∏è",x=keyboard and "x" or "‚ùé"}

  poke(0x5f80,4)
  cartdata("tobiasvl_meteor_night")
  s=cocreate(say)
  meteors={}
  music(0)
  states={
    intro=0,
    title=1,
    game_over=2
  }

  sunset_colors={
    --12,
    {9,11},
    {14,11},
    {8,3},
    {2,3},
    --13,
    --1,
    {0,13}
  }
  state=states.intro

  cam_x,cam_y=canvas/2,canvas/2
  frame_counter=0
  anim_frame=0
  counter=1
  credits_y=0

  -- mouse stuff
  poke(0x5f2d,1)
  mouse={
    x=stat(32),
    y=stat(33),
    pressed_x=0,
    pressed_y=0,
    down=false}
  last_mouse=0
  last_x=0
  last_y=0
  pressed_x=0
  pressed_y=0
  touch=false
  analog={
    x=0,
    y=0,
    old_x=0,
    old_y=0}

  chilly=true
  chill=0

  meteors_seen=0
  meteors_high=dget(1)
  spr_x=40

  stars={}
  for i=1,stars_amount do
    local x,y=flr(rnd(canvas)),flr(rnd(canvas))
    add(stars,{x=x,y=y})
    if x<128 then
      add(stars,{x=x+canvas,y=y})
    elseif y<128 then
      add(stars,{x=x,y=y+canvas})
    end
    if x<128 and y<128 then
      add(stars,{x=x+canvas,y=y+canvas})
    end
  end

  moon={x=flr(rnd(canvas-128-16))+128,y=flr(rnd(canvas-128-16))+128}
end
