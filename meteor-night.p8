--draw
function _draw()
  if state==states.intro then
    frame_counter+=1
    anim_frame+=0.5
    if counter==#sunset_colors then
      spr_x-=1
    elseif frame_counter%25==0 then
      counter+=1
    end
    if spr_x+40<0 then
      title_y=cam_y
      state=states.title
    end
    cls(sunset_colors[counter][1])

    if counter>=#sunset_colors-1 then
      print_stars()
    end

    rectfill(0,100,128,128,sunset_colors[counter][2])

    spr(161+(flr(anim_frame)%6)*2,spr_x,78,2,3)
    pal(12,1)
    spr(161+(flr((anim_frame)+4)%6)*2,spr_x+24,78,2,3)
    pal()
  elseif state==states.title then
    cls()

    print_stars()

    camera(cam_x,title_y)

    if (chilly) center("‚òÖ"..(scroll and meteors_seen or meteors_high),cam_y+40,7)

    local press="press "..buttons.x
    if not scroll then
      if (stat(95)%2==0) outline(press,cam_x,title_y+30,12,7,true)
      if chilly then
        spr(176,cam_x+15,title_y+20)
        outline("chill mode (press "..buttons.o..")",0,title_y+20,12,7,true)
      else
        outline("chill out mode (press "..buttons.o..")",0,title_y+20,1,6,true)
      end
    end
    palt(0,false)
    sspr(0,0,127,127,cam_x+0,cam_y+49)
    palt()
  elseif state==states.play then
    cls()
    camera(cam_x,cam_y)

    if debug then
      rect(cam_x,cam_y,cam_x+127,cam_y+127,8)
      color(2)
      print("mouse.y:"..mouse.y,cam_x+1,cam_y+1)
      print("cam_y:"..cam_y,cam_x+1,cam_y+7)
      print("mouse.x:"..mouse.x,cam_x+1,cam_y+13)
      print("cam_x:"..cam_x,cam_x+1,cam_y+19)
      print("saying:"..(s and costatus(s) or "nil"),cam_x+1,cam_y+25)
      print("mem:"..stat(0),cam_x+1,cam_y+31)
      print("fps:"..stat(7).."/"..stat(9),cam_x+1,cam_y+37)
      color(stat(1)>=1 and 8 or 2)
      print("cpu:"..stat(1),cam_x+1,cam_y+43)
      print("mousepress:"..tostr(stat(34)),cam_x+1,cam_y+49)
    end

    print_stars()

    palt(0,false)
    circfill(moon.x,moon.y,16,7)
    circfill(moon.x+10,moon.y+10,16,0)
    palt()

    for meteor_data in all(meteors) do
      local m=meteor_data.thread
      if costatus(m)!="dead" then
        coresume(m)
      else
        del(meteors,meteor_data)
      end
    end

    if s and costatus(s)!="dead" then
      coresume(s)
    end

    if chilly then
      spr(176,cam_x+40,cam_y+110)
      rect(cam_x+48,cam_y+110,cam_x+80,cam_y+116,7)
      rectfill(cam_x+49,cam_y+111,cam_x+79,cam_y+115,0)
      rectfill(cam_x+49,cam_y+111,cam_x+49+min(30,flr(chill)),cam_y+115,12)
    end

    center("‚òÖ"..meteors_seen,cam_y+120,7)

    spr(160,mouse.x,mouse.y)
  elseif state==states.game_over then
    cls()

    print_stars()

    camera(cam_x,credits_y)

    local co=300
    center("meteor night",cam_y+co,7)
    co+=48
    center("by tobias v. langhoff",cam_y+co,7)
    co+=64
    center("music by robby duguay",cam_y+co,7)
    co+=64
    center("based on an idea by",cam_y+co,7)
    co+=48
    center("eric caoili,",cam_y+co,7)
    co+=48
    center("jc fletcher",cam_y+co,7)
    co+=48
    center("and ashley davis",cam_y+co,7)
    co+=64
    center("with artwork by ashley davis",cam_y+co,7)
    co+=24
    palt(0,false)
    sspr(0,0,127,127,cam_x,cam_y+co)
    palt()
  end
end
